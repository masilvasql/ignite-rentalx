                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/definitions/Specification"
                            }
                        }
                    }
                },
                "responses":{
                    "201":{
                        "description":"created"
                    },
                    "500":{
                        "description":"Speification already exists"
                    }
                }
            }
        }
    },
    "definitions":{
        "Specification":{

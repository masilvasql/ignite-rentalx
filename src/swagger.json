            "post":{
                "tags":["Category"],
                "summary":"Create a category",
                "description":"Create a new Category",
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"objecte"
                            },
                            "properties":{
                                "name":{
                                    "type":"string"
                                },
                                "description":{
                                    "type":"string"
                                }
                            },
                            "example":{
                                "name":"categoryTeste",
                                "description":"Description Sample"
                            }
                        }
                    }
                },
                "responses":{
                    "201":{
                        "description":"created"
                    },
                    "500":{
                        "description":"Category already exists"
                    }
                }
            },
            "get":{
                "tags":["Category"],
                "sumary":"List all categories",
                "description":"Lista all categories",
                "responses":{
                    "200":{
                        "description":"Success",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"array",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "name":{
                                                "type":"string"
                                            },
                                            "description":{
                                                "type":"string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/categories/import":{
            "post":{
                "tags":["Category"],
                "summary":"Upload a new category",
                "description":"Upload a new Category",
                "requestBody":{
                    "content":{
                        "multipart/form-data":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "file":{
                                        "type":"string",
                                        "format":"binary"
                                    }
                                }
                            }
                        }
